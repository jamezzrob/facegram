<%= simple_form_for(@post) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :photo %>
    <%= f.input :caption %>
    <%= f.input :location %>
    <!-- <%= f.association :user %> -->
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
